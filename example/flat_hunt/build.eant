<?xml version="1.0"?>

<project name="flat_hunt" default="help">

  <description>
    description: "Eiffel Ant file for 'flat_hunt'"
    system: "Flat Hunt v2"
    copyright: "Copyright (c) 2004, Ursina Caluori and others"
    license: "Eiffel Forum License v2 (see forum.txt)"
    date: "$Date$"
    revision: "$Revision$"
  </description>

  <inherit>
    <parent location="${EM}/misc/em_application.eant">
      <redefine target="init_system"/>
    </parent>
  </inherit>
  
  <!-- Implementation -->

  <target name="init_system">
    <set name="system" value="flat_hunt"/>
    <set name="system_dir" value="."/>
  </target>
</project>
